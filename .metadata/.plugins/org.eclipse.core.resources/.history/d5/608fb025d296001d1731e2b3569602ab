
public class Quiz_01 {
	public static void main(String[] args) {
		byte b = 10;
		short s = 20;
		char c = 'A';
		int i;
		float f;
		long l = 100L;
		
		s = b;	// byte -> short : 자동형변환 
		c = (char)b;	// byte -> char : promotion 
		s = (short)c;	// char -> short : 
		c = (char)s;
		i = (int)100L;
		l = 500;
		f = l;
		f = (float)5.11;
	}
}
